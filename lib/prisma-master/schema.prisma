generator client {
  provider      = "prisma-client-js"
  output        = "../lib/prisma-master"
  binaryTargets = ["native", "rhel-openssl-3.0.x", "windows"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Project {
  id            Int      @id @default(autoincrement())
  name          String
  databaseUrl   String
  neonProjectId String
  createdAt     DateTime @default(now())
}
