// prisma/projectSchema.prisma
datasource db {
  provider = "postgresql"
  url      = env("PROJECT_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../lib/prisma-project"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
